<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Attendees_Should_Not_Exceed_The_Quota</fullName>
    <active>true</active>
    <errorConditionFormula>Attendees_Count__c &gt;  Attendees_Quota__c</errorConditionFormula>
    <errorMessage>Attendees Count should not be greater than the Attendees Quota</errorMessage>
</ValidationRule>
