<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Due_Date_Five_Days_After</fullName>
    <active>true</active>
    <errorConditionFormula>Due_Date__c &lt;= ( TODAY() + 5)</errorConditionFormula>
    <errorMessage>Due date cannot bee sooner than 5 days after creation</errorMessage>
</ValidationRule>
